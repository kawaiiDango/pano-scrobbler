<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled -->
    <item android:color="@android:color/transparent" android:state_enabled="false"/>

    <!-- Selected -->
    <item android:color="#40FFFFFF" android:state_selected="true"/>
    <item android:color="#40FFFFFF" android:state_checked="true"/>

    <!-- Not selected -->
    <item android:color="#0DFFFFFF"  />
</selector>
